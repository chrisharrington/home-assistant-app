{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react",
    "moduleResolution": "node",
    "baseUrl": "./",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "paths": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}